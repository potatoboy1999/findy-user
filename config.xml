<?xml version='1.0' encoding='utf-8'?>
<widget id="com.findy.findyApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Findy-User</name>
    <description>
        Findy, encuentra todas tus tiendas
    </description>
    <author email="soporte@abbost.com" href="http://abbost.com">
        ABBOST DEVELOPMENT TEAM
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="~7.1.4" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" spec="^4.2.2">
        <variable name="OKHTTP_VERSION" value="3.10.0" />
        <variable name="LOCATION_USAGE_DESCRIPTION" value="This app requires access to your location for navigation purposes" />
    </plugin>
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <preference name="phonegap-version" value="cli-8.1.1" />
</widget>
